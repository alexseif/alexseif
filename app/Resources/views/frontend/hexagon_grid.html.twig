{% extends 'base.html.twig' %}
{% block stylesheets %}
  {{ parent() }}
  {% stylesheets filter="scssphp" output="css/app.css"
            "css/hexagon_grid.scss"
  %}
  <link rel="stylesheet" href="{{ asset_url }}" />
  {% endstylesheets %}
{% endblock %}
{% block body %}
  <div class="hex-container">
    {% for portfolio in portfolios %}
      {% if (((loop.index % 5) == 1) or ((loop.index % 5) == 4))%}
        <div class="hex-grid">
        {% endif %}
        <a href="#" class="-hex">
          <div class="-hex-wrap">
            <div class="-card -front" style="background-color: #1D1F29;">
              <div class="-inner">
                <h3>{{ portfolio.title }} - {{ loop.index }} - {{ ((loop.index) % 5) }}</h3>
                <p>{{ loop.index }}</p>
              </div>
              <div class="-clip -left"></div>
              <div class="-clip -right"></div>
            </div>
            <div class="-card -back .background" style="">
              <div class="-inner">
                <h3>Title</h3>
                <p>description</p>
              </div>
              <div class="-clip -left"></div>
              <div class="-clip -right"></div>
            </div>
          </div>
        </a>
        {% if (((loop.index % 5) == 3) or ((loop.index % 5) == 0))%}
        </div>
      {% endif %}
    {% endfor %}
