<!DOCTYPE html>
<html class='full'>
  <head>
    <meta charset="UTF-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
      <link rel="stylesheet" type="text/css" href="//maxcdn.bootstrapcdn.com/bootswatch/3.3.7/darkly/bootstrap.min.css" />
      <link href="https://fonts.googleapis.com/css?family=Lato:100,300,400,700" rel="stylesheet">
      <link rel="stylesheet" type="text/css" href="/css/admin/custom.css" />
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
  </head>
  <body>
    {% block body %}
      {% include 'AdminBundle:Default:nav.html.twig' %}
      {% block content %}
        <div class="container">
          {% block container %}
            <div class="page-actions">
              {% block pageActions %}{% endblock %}
            </div>
            {% block adminPanelFormStart %}{% endblock %}
            {% block adminPanel %}
              <div class="panel {% block adminPanelClass %}panel-default{% endblock %}">
                <div class="panel-heading">
                  {% block adminPanelHeading %}
                    <span class="panel-title">
                      <strong>{% block adminPanelTitle %}Title{% endblock %}</strong>
                      <small>{% block adminPanelTitleSmall %}{% endblock %}</small>
                      <span class="badge">{% block adminPanelTitleBadge %}{% endblock %}</span>
                    </span>
                    <div class="pull-right">
                      {% block adminPanelHeadingRight %}{% endblock %}
                    </div>
                  {% endblock %}
                </div>
                {% block adminPanelBody %}{% endblock %}
                {% block adminPanelFooter %}{% endblock %}
              </div>
            {% endblock %}
            {% block adminPanelFormEnd %}{% endblock %}
          {% endblock %}
        </div>
      {% endblock %}
    {% endblock %}
    {% block javascripts %}
      <script type="text/javascript" src="//code.jquery.com/jquery-2.2.4.min.js"></script>
      <script type="text/javascript" src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
    {% endblock %}
  </body>
</html>
